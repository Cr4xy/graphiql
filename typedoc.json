{
  "$schema": "https://typedoc.org/schema.json",
  "name": "GraphQL Official LSP Documentation",
  "theme": "default",
  "darkHighlightTheme": "dark-plus",
  "out": "packages/graphiql/typedoc",
  "tsconfig": "resources/tsconfig.build.json",
  "entryPointStrategy": "packages",
  "includeVersion": true,
  "entryPoints": ["packages/*"],
  "exclude": [
    "**/__tests__/**",
    "*.spec.*",
    "**/*.stories.tsx",
    "*-test.*",
    "**/cypress/**"
  ],
  "validation": {
    "notExported": false,
    "notDocumented": false,
    "invalidLink": false
  },
  "readme": "./README.md",
  "externalSymbolLinkMappings": {
    "graphql": {
      "GraphQLSchema": "https://graphql-js.org/api/class/GraphQLSchema",
      "DocumentNode": "https://graphql-js.org/api/#DefinitionNode",
      "TypeNode": "https://graphql-js.org/api#TypeNode"
    },
    "graphql-config": {
      "GraphQLConfig": "https://the-guild.dev/graphql/config/docs/library/graphql-config",
      "GraphQLProjectConfig": "https://the-guild.dev/graphql/config/docs/library/graphql-project-config"
    }
  }
}
