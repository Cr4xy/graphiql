{
  "name": "@graphiql/plugin-utils",
  "description": "shared tooling for building graphiql plugins",
  "version": "0.0.1",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "cjs/vite-plugin.js",
  "module": "dist/vite-plugin.js",
  "types": "types/vite-plugin.d.ts",
  "exports": {
    "./base.tsconfig.json": "./base.tsconfig.json",
    "./vite.config.tsconfig.json": "./vite.config.tsconfig.json",
    ".": {
      "types": "./types/vite-plugin.d.ts",
      "require": "./cjs/vite-plugin.js",
      "import": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "*.json",
    "dist",
    "cjs"
  ],
  "scripts": {
    "build": "tsc && tsc --module CommonJS --outDir cjs"
  },
  "dependencies": {
    "vite": "^4.4.2",
    "vite-plugin-dts": "^3.1.1",
    "vite-plugin-svgr": "^3.2.0",
    "postcss-nesting": "12.0.0",
    "@vitejs/plugin-react": "^4.0.2",
    "@graphiql/react": "^0.18.0"
  }
}
